<div class="container"></div>
<table class="table table-hover full_width">
  <tbody>
  <% @posts.each do |post| %>
      <tr class="clickable full_width" data-location="<%= url_for post %>">
        <td><%= post.title %></td>
        <td><%= post.created_at %></td>
        <td><%= post.user.email %></td>
      </tr>
  <% end %>
  </tbody>
</table>
